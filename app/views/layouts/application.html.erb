<!DOCTYPE html>
<html>
  <head>
    <title>Snsapp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <header>
      <%= link_to image_tag('snsapp_logo.png',class:'header-logo'), posts_path %>
    </header>
    <div class="main">
      <%= yield %>
    </div>
  </body>
</html>
