<div class="">
  <h1>ホーム</h1>
</div>
<div class="post_table">
  <%= form_with(model:@post, local:true) do |form| %>
    <div class="">
      <%= form.text_field label:content %>
    </div>
    <div class="">
      <%= form.submit %>
    </div>
  <% end %>
</div>
<div class="">
  <% @posts.each do |post| %>
    <div class="log_table">
      <div>
        <p><%= post.content %></p>
      </div>
      <div class="">
        <span><%= post.created_at %></span>
      </div>
    </div>
  <% end %>
</div>
